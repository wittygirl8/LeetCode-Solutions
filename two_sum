class Solution():
    def twoSum(self, nums, target):
        li = []
        for i in range(0,len(nums)):
            for j in range(i+1,len(nums)):
                if nums[i]+nums[j] == target:
                    li.append(i)
                    li.append(j)
        return li
                


if __name__ == '__main__':
    # begin
    s = Solution()
    print (s.twoSum([3, 2, 4], 6))

        